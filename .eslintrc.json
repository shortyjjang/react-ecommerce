{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off",
        "no-cond-assign": "off",
        "no-case-declarations": "off",
        "no-constant-condition": "off",
        "no-empty": "off",
        "no-undef": "off",
        "no-prototype-builtins": "off",
        "no-extra-semi": "off",
        "no-debugger": "warn",
        "react/prop-types": "off",
        "react/no-unescaped-entities": "off",
        "react/jsx-key": "off",
        "react/no-unknown-property": "off",
        "react/jsx-no-undef": "off"
    }
}
